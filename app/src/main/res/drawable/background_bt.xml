    <?xml version="1.0" encoding="utf-8"?>-->

<selector xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">

    <item android:state_pressed="true">

        <!-- 定义当button 处于pressed 状态时的形态。-->

        <shape>

            <solid android:color="@color/aquamarine_blue_dark" />

            <corners android:radius="10dp" />

        </shape>

    </item>
    <item android:state_pressed="false">

        <!-- 定义当button 未处于pressed 状态时的形态。-->

        <shape>
            <solid android:color="@color/aquamarine_blue" />

            <corners android:radius="10dp" />

        </shape>

    </item>


</selector>